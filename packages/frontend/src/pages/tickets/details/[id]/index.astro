import InnerHeader from "../../_components/innerHeader/InnerHeader.astro";
import InnerMain from "../../_components/innerMain/InnerMain.astro";
const { id } = Astro.params;
---

<Layout>
    <Header />
    <Main>
        <InnerHeader>
            <h2>Ticket details page with id=<strong>{id}</strong></h2>
        </InnerHeader>
        <InnerMain />
    </Main>
</Layout>
